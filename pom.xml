    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-surefire-plugin</artifactId>
                <version>2.19.1</version>
                <configuration>
                    <suiteXmlFiles>
                        <suiteXmlFile></suiteXmlFile>
                    </suiteXmlFiles>
                    <systemPropertyVariables>
                        <Environment>Local</Environment>
                        <PlatformVersion>9.3</PlatformVersion>
                        <Language>en</Language>
                        <Country>US</Country>
                        <PlatformName>iOS</PlatformName>
                        <DeviceName>iPhone6</DeviceName>
                        <AppiumVersion>1.5.3</AppiumVersion>
                        <DeviceType>phone</DeviceType>
                        <DeviceOrientation>portrait</DeviceOrientation>
                        <AppPath></AppPath>
                        <BrowserName></BrowserName>
                        <driverName>MobileApp</driverName>
                        <testObjectUser>dias_p</testObjectUser>
                        <testObjectApiKey></testObjectApiKey>
                        <testObjectDevice></testObjectDevice>
                        <appName></appName>
                    </systemPropertyVariables>
                    <properties>
                        <property>
                            <name>usedefaultlisteners</name>
                            <value>false</value>
                        </property>
                        <property>
                            <name>listener</name>
                            <value>com.subway.qa.Util.ExtentReporterNG</value>
                            <value>org.testng.reporters.EmailableReporter</value>
                        </property>
                        <workingDirectory>target/</workingDirectory>
                    </properties>

                </configuration>
            </plugin>
            <plugin>
                <artifactId>maven-compiler-plugin</artifactId>
                <configuration>
                    <encoding>iso-8859-1</encoding>
                    <source>1.7</source>
                    <target>1.7</target>
                </configuration>
            </plugin>
        </plugins>
    </build>

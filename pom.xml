<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>com.fwh.qa</groupId>
    <artifactId>mobileApp</artifactId>
    <version>1.0-SNAPSHOT</version>
    <url>http://maven.apache.org</url>
    <dependencies>

            <dependency>
                <groupId>com.fwh.qa</groupId>
                <artifactId>automationFramework</artifactId>
                <version>2.2.38-SNAPSHOT</version>
            </dependency>
    <dependency>
            <groupId>com.fwh.qa</groupId>
            <artifactId>ServiceFramework</artifactId>
            <version>LATEST</version>
        </dependency>

    </dependencies>

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-surefire-plugin</artifactId>
                <version>2.19.1</version>
                <configuration>
                    <suiteXmlFiles>
                        <suiteXmlFile></suiteXmlFile>
                    </suiteXmlFiles>
                    <systemPropertyVariables>
                        <Environment>Local</Environment>
                        <PlatformVersion>9.3</PlatformVersion>
                        <Language>en</Language>
                        <Country>US</Country>
                        <PlatformName>iOS</PlatformName>
                        <DeviceName>iPhone6</DeviceName>
                        <AppiumVersion>1.5.3</AppiumVersion>
                        <DeviceType>phone</DeviceType>
                        <DeviceOrientation>portrait</DeviceOrientation>
                        <AppPath></AppPath>
                        <BrowserName></BrowserName>
                        <driverName>MobileApp</driverName>
                        <testObjectUser>dias_p</testObjectUser>
                        <testObjectApiKey></testObjectApiKey>
                        <testObjectDevice></testObjectDevice>
                        <appName></appName>
                    </systemPropertyVariables>
                    <properties>
                        <property>
                            <name>usedefaultlisteners</name>
                            <value>false</value>
                        </property>
                        <property>
                            <name>listener</name>
                            <value>com.subway.qa.Util.ExtentReporterNG</value>
                            <value>org.testng.reporters.EmailableReporter</value>
                        </property>
                        <workingDirectory>target/</workingDirectory>
                    </properties>

                </configuration>
            </plugin>
            <plugin>
                <artifactId>maven-compiler-plugin</artifactId>
                <configuration>
                    <encoding>iso-8859-1</encoding>
                    <source>1.7</source>
                    <target>1.7</target>
                </configuration>
            </plugin>
        </plugins>
    </build>

    <distributionManagement>
        <snapshotRepository>
            <id>automationFramework</id>
            <url>http://10.50.120.151:8090/repository/automationFramework/</url>
        </snapshotRepository>
    </distributionManagement>
    <repositories>
        <repository>
            <id>automationFramework</id>
            <name>automationFramework</name>
            <url>http://10.50.120.151:8090/repository/automationFramework/</url>
            <releases>
                <enabled>true</enabled>
            </releases>
            <snapshots>
                <enabled>true</enabled>
            </snapshots>
        </repository>
        <repository>
            <id>ServiceFramework</id>
            <name>ServiceFramework</name>
            <url>http://10.50.120.151:8090/repository/ServiceFramework/</url>
            <releases>
                <enabled>true</enabled>
            </releases>
            <snapshots>
                <enabled>true</enabled>
            </snapshots>
        </repository>

    </repositories>
    <pluginRepositories>
        <pluginRepository>
            <id>automationFramework</id>
            <name>automationFramework</name>
            <url>http://10.50.120.151:8090/repository/automationFramework/</url>
            <releases>
                <enabled>true</enabled>
            </releases>
            <snapshots>
                <enabled>true</enabled>
            </snapshots>
        </pluginRepository>

        <pluginRepository>
            <id>ServiceFramework</id>
            <name>ServiceFramework</name>
            <url>http://10.50.120.151:8090/repository/ServiceFramework/</url>
            <releases>
                <enabled>true</enabled>
            </releases>
            <snapshots>
                <enabled>true</enabled>
            </snapshots>
        </pluginRepository>

    </pluginRepositories>

</project>

